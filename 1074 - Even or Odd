#include<iostream>
using namespace std;
int main()
{
int a,n,m;
cin>>n;
for(int i=1; i<=n; i++)
{
cin>>m;
if(m==0)
    cout<<"NULL"<<endl;
    else if(m<=0 && m%2==0)
        cout<<"EVEN NEGATIVE"<<endl;
    else if(m<=0 && m%2==-1)
        cout<<"ODD NEGATIVE"<<endl;
    else if(m>=0 && m%2==0)
        cout<<"EVEN POSITIVE"<<endl;
     else if(m>=0 && m%2==1)
        cout<<"ODD POSITIVE"<<endl;
}
return 0;
}
